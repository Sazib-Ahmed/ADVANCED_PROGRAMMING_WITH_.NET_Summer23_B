
@{
    ViewBag.Title = "Education";
}
<div>
    <a href="/portfolio/index">Home</a>
    <a href="/portfolio/profile">Profile</a>
    <a href="/">Education</a>
    <a href="/portfolio/projects">Projects</a>
    <a href="/portfolio/references">References</a>
</div>

<h2>Education</h2>
<br>

<ul>
    @foreach (var item in ViewBag.Edus)
    {
        <li>
            @item.Title
            <ul>
                <li>@item.Institution</li>
                <li>@item.Year</li>
            </ul>
        </li>
    }
</ul>